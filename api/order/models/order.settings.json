{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "total": {
      "type": "biginteger",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ],
      "default": "unpaid"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "note": {
      "type": "text"
    },
    "cart": {
      "collection": "cart-item",
      "via": "order"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    }
  }
}
